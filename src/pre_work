from __future__ import division
import sys

import ngram
import nltk
import string
from config import *

pjoin = os.path.join


def count():
    """
    Results:

    train: 11701890
    existed: 355
    dev: 1000
    existed ratio: 0.355
    """
    max_count = 0;
    fileName = train_click_log
    """fileName = '/home/lql/python/input.txt'"""
    with open(train_click_log, 'r') as f:
        for line in f:
            arr = line.strip().split('\t')
            click = string.atol(arr[2].strip())
            if click > max_count:
                max_count = click
    print max_count
if __name__ == "__main__":
    # query_existed()
    # query_search_test()
    count()
